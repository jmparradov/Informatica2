#include "character.h"

character::character()
{

    QMovie *movie = new QMovie("../Gravity_Bike/sources/character/hittin_nbg.gif");
    QLabel *label = new QLabel(this);

    label->setMovie(movie);
    movie->start();
    setAttribute( Qt::WA_TranslucentBackground, true );

    additem(label);

    //make corpus focusable
    setFlag(QLabel::ItemIsFocusable);
    setFocus();
}
